[Unit]
Description=Read bittware-250's VPD via ibm-parser from EEPROM
Wants=mapper-wait@-xyz-openbmc_project-inventory.service
After=mapper-wait@-xyz-openbmc_project-inventory.service
Wants=op-power-start@0.target
After=op-power-start@0.target
Conflicts=obmc-chassis-poweroff@0.target

[Service]
ExecStart=/usr/bin/env enable-ibm-parser.sh
SyslogIdentifier=ibm-read-vpd
Type=dbus
BusName=xyz.openbmc_project.250socvpd.manager

[Install]
WantedBy=obmc-chassis-poweron@0.target

